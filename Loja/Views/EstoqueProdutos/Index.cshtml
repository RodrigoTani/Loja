@model IEnumerable<Loja.Models.EstoqueProdutos>
<div class="container">
    <p>
        @Html.ActionLink("Adiciolnar Novo", "Create",null, new { @class="btn btn-primary"})
    </p>
    <table class="table">
        <tr>
            <th>@Html.DisplayNameFor(model => model.AdicionarJogo.Titulo)</th>
            <th>@Html.DisplayNameFor(model => model.fornecedor1.RazaoSocial)</th>
            <th>@Html.DisplayNameFor(model => model.PorcentagemPrecificacao)</th>
            <th>@Html.DisplayNameFor(model => model.Custo)</th>
            <th>@Html.DisplayNameFor(model => model.Quantidade)</th>
            <th>@Html.DisplayNameFor(model => model.Ativo)</th>
            <th>@Html.DisplayNameFor(model => model.DataCadastro)</th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.AdicionarJogo.Titulo)</td>
                <td>@Html.DisplayFor(modelItem => item.fornecedor1.RazaoSocial)</td>
                <td>@Html.DisplayFor(modelItem => item.PorcentagemPrecificacao)</td>
                <td>@Html.DisplayFor(modelItem => item.Custo)</td>
                <td>@Html.DisplayFor(modelItem => item.Quantidade)</td>
                <td>@Html.DisplayFor(modelItem => item.Ativo)</td>
                <td>@Html.DisplayFor(modelItem => item.DataCadastro)</td>
                <td>@Html.ActionLink("Editar", "Edit", null, new { id = item.Id }, new { @class = "btn btn-primary" })</td>
                <td>@Html.ActionLink("Detalhes", "Details", null, new { id = item.Id }, new { @class = "btn btn-default" })</td>
                <td>@Html.ActionLink("Deletar", "Delete", null, new { id = item.Id }, new { @class = "btn btn-danger" })</td>
            </tr>
        }

    </table>
</div>